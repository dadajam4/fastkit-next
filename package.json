    "@types/fs-extra": "^11.0.1",
    "@types/node": "^18.11.19",
    "@types/prismjs": "^1.26.0",
    "@vanilla-extract/css": "^1.10.0",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vue/runtime-core": "^3.2.47",
    "@vueuse/head": "^1.1.23",
    "bezier-easing": "^2.1.0",
    "cookie": "^0.5.0",
