    "typescript": "^5.0.2",
    "vite": "^4.1.1",
    "vitest": "^0.29.7",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "packageManager": "pnpm@7.30.1",
  "engines": {
    "node": ">=18.13.0",
    "npm": "use pnpm",
    "pnpm": ">=7",
    "yarn": "use pnpm"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "typescript",
        "eslint",
        "prettier",
